{
    "defaultSeverity": "error",
    "extends": [
        "tslint:recommended"
    ],
    "jsRules": {},
    "rules": {
        "max-line-length": {
            "options": [120]
        },
        "max-classes-per-file" : false,
        "interface-name":false,
        "object-literal-sort-keys":false,
        "no-console": {
            "severity": "",
            "options": [
                "debug",
                "info",
                "log",
                "time",
                "timeEnd",
                "trace"
            ]
        }
    },
    "rulesDirectory": []
}